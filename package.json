{
  "name": "storefront_backend",
  "version": "0.1.0",
  "description": "back end for a store database",
  "main": "index.js",
  "scripts": {
    "start": "npx ts-node src/server.ts",
    "watch": "tsc-watch --esModuleInterop src/server.ts --outDir ./dist --onSuccess node \"./dist/server.js\"",
    "test": "set ENV=test && db-migrate --env test up && jasmine-ts  --config=./spec/support/jasmine.json && db-migrate --test dev down --count 10",
    "tsc": "tsc",
    "lint": "eslint src/ --ext .ts",
    "prettier": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "build": "npx tsc",
    "migrate-test": "db-migrate --env test up && db-migrate up",
    "migrate": "db-migrate --env dev up && db-migrate up",
    "jasmine": "jasmine-ts  --config=./spec/support/jasmine.json"
  },
  "author": "Ahmed Nabil",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.2",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "jsonwebtoken": "^8.5.1",
    "node": "^10.24.1",
    "pg": "^8.7.3",
    "postgres": "^1.0.2",
    "yarn": "^1.22.18"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "^3.10.4",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/pg": "^7.14.11",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "db-migrate": "^0.11.13",
    "db-migrate-pg": "^1.2.2",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jasmine": "^3.99.0",
    "jasmine-spec-reporter": "^6.0.0",
    "jasmine-ts": "^0.3.3",
    "prettier": "^2.6.1",
    "supertest": "^6.2.2",
    "ts-node": "^9.1.1",
    "tsc-watch": "^4.6.2",
    "typescript": "^4.6.3"
  }
}
